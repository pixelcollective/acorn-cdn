{
  "name": "tiny-pixel/acorn-cdn",
  "type": "package",
  "license": "MIT",
  "description": "Lightweight CDN mechanism for Acorn projects",
  "authors": [
    {
      "name": "Kelly Mears",
      "email": "kelly@tinypixel.dev"
    }
  ],
  "keywords": [
    "wordpress",
    "acorn",
    "sage"
  ],
  "support": {
    "issues": "https://github.com/pixelcollective/acorn-cdn/issues"
  },
  "autoload": {
    "psr-4": {
      "TinyPixel\\Acorn\\CDN\\": "src/"
    }
  },
  "require": {
    "php": ">=7.3"
  },
  "extra": {
    "acorn": {
      "providers": [
        "TinyPixel\\Acorn\\CDN\\Providers\\CDNServiceProvider"
      ]
    }
  }
}
